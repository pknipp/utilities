{% extends "layout.html" %}

{% block content %}
<div class="text-center">
    <h3>
        Utilities provided by this API
    </h3>
</div>
<div>
    <ul>
        {% for utility in utilities %}
            <li><a href="{{utility.name}}">{{ utility.pretty }}</a>: {{utility.description|raw}}</li>
        {% endfor %}
    </ul>
</div<

{% endblock %}
